		"postcss-preset-env": "^8.5.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.65.1",
		"scrape-it": "^6.0.1",
		"sharp": "^0.32.6",
		"svelte": "^4.2.1",
		"svelte-check": "^3.4.6",
		"svelte-lazy-loader": "^1.0.0",
		"svelte-preprocess": "^5.0.4",
